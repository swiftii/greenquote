{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && yarn install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/widgets/(.*)",
      "destination": "/widgets/$1"
    },
    {
      "source": "/pro/(.*)",
      "destination": "/pro/$1"
    },
    {
      "source": "/configs/(.*)",
      "destination": "/configs/$1"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server.py"
    },
    {
      "src": "/widgets/(.*)",
      "dest": "/widgets/$1"
    },
    {
      "src": "/pro/(.*)",
      "dest": "/pro/$1"
    },
    {
      "src": "/configs/(.*)",
      "dest": "/configs/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
